<Window x:Class="UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI"
        mc:Ignorable="d"
        Title="WPFDemo" Height="450" Width="800" FontSize="15" SizeChanged="Window_SizeChanged" StateChanged="Window_StateChanged" MinWidth="800" MinHeight="450">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition x:Name="WindowWidth" Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition x:Name="WindowHeight" Height="*"/>
        </Grid.RowDefinitions>
        <Rectangle x:Name="Rectangle" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        <Frame x:Name="Frame" Height="auto" Width="auto" Grid.Column="0" Grid.Row="0" Margin="1,1,1,1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Navigated="Frame_Navigated" NavigationUIVisibility="Hidden">
        </Frame>
    </Grid>
</Window>
